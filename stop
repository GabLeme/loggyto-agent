sudo systemctl stop loggyto-agent.service && \
sudo systemctl disable loggyto-agent.service && \
sudo rm -f /etc/systemd/system/loggyto-agent.service && \
sudo systemctl daemon-reexec && \
sudo systemctl daemon-reload && \
sudo rm -rf /opt/loggyto && \
echo "Loggyto Agent removido com sucesso."


